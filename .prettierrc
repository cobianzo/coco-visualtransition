{
	"semi": true,
	"singleQuote": true,
	"singleAttributePerLine": false,
	"htmlWhitespaceSensitivity": "css",
	"printWidth": 150,
	"trailingComma": "es5",
	"plugins": ["prettier-plugin-organize-imports"],
	"tabWidth": 4,
	"overrides": [
			{
					"files": "**/*.yml",
					"options": {
							"tabWidth": 2
					}
			}
	]
}
